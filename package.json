{
  "name": "endymion",
  "version": "1.6.0",
  "description": "Endymion library for augmented reality",
  "main": "./lib/endymion.js",
  "types": "./lib/endymion.d.ts",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "setup": "npm install",
    "build:compiled": "npx webpack --config webpack.config.js",
    "build": "tsc --project tsconfig.module.json",
    "watch": "tsc --project tsconfig.module.json --watch",
    "test": "npm run build && mocha --require ts-node/register src/**/*.spec.ts",
    "test:ci": "mocha --require ts-node/register src/**/*.spec.ts --reporter mocha-junit-reporter",
    "doc": "typedoc src/endymion.ts --out ./docs",
    "publish": "npm publish --registry https://npm.endymion.tech/"
  },
  "keywords": [
    "augmented",
    "reality"
  ],
  "author": {
    "name": "Endymion"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.2",
    "chai": "^4.3.10",
    "clean-webpack-plugin": "^4.0.0",
    "jsdom": "^22.1.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.8.2",
    "rxjs": "^7.8.1",
    "sinon": "^16.1.0"
  }
}
